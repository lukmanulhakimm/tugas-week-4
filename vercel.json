{
    "builds":[
        {
        "src":"./index.js",
        "use":"@vercel/node"
        }
    ],
    "rewrites":[
        {
        "source":"/(.*)",
        "destination":"/"

        }
    ],
    "headers":[
        {
         "source":"/(.*)",
         "headers":[
            {"key": "Access-Control-Allow-Credentials","values":"true"},
            {"key": "Access-Control-Allow-Origin","values":"*"},
            {"key": "Access-Control-Allow-Methods","values":"GET,OPTIONS,PATCH,DELETE,POST,PUT"},
            {"key": "Access-Control-Allow-Headers","values":"X-CSRF-Token, X-Requested-With,Accept,Accept-Version,Content-Length,Content-MD5,Content-Type,Date,X-Api-Version"}

         ]
        }
    ]
}
